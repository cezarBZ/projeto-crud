<!DOCTYPE html>
<html lang="pt-br">
<%- include('partials/head.ejs')%>

<body>
    <%- include('partials/navbar.ejs')%>
    <h1><%= title %></h1>
    <ul>
        <% users.forEach(user => { %>
         <li>
             <%= user.name %> - <%= user.email %> - <%= user.age %> anos -
             <a href="/edit?id=<%= user._id %>">[editar]</a>
         </li>
        <% }) %>

    </ul>
    <%- include('partials/foot.ejs')%>
</body>
</html>